<Window x:Class="hardy_analyseOBS.Analyzed"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:hardy_analyseOBS"
        mc:Ignorable="d"
        Title="Analyzed" Height="600" Width="800"
        ResizeMode="NoResize">
    <Grid>
        <Label x:Name="cpu_label" Content="CPU" HorizontalAlignment="Left" Margin="10,11,0,0" VerticalAlignment="Top" FontSize="100"/>
        <Label x:Name="ram_label" Content="RAM" HorizontalAlignment="Left" Margin="270,11,0,0" VerticalAlignment="Top" FontSize="100"/>
        <Label x:Name="gpu_label" Content="GPU" HorizontalAlignment="Left" Margin="550,11,0,0" VerticalAlignment="Top" FontSize="100"/>
    </Grid>
</Window>
